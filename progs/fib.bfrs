fn main() {
    let y : u8 = 10;
    while y {
        println(fib(y));
        y = y - 1;
    };
    println(fib(y));
}

fn fib(x: u8) -> u8 {
    if x {
        let x_minus_1 : u8 = x - 1;
        if x_minus_1 {
            let x_minus_2 : u8 = x_minus_1 - 1;
            let f1 : u8 = fib(x_minus_1);
            let f2 : u8 = fib(x_minus_2);
            f1 + f2
        } else {
            1
        }
    } else {
        1
    }
}
